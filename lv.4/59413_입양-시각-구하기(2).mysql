-- 0 ~ 23 시 테이블 생성. COUNT = 0 인 시도 컬럼에 나올 수 있도록
WITH RECURSIVE HOUR AS (
    SELECT 0 AS h
    UNION ALL
    SELECT h + 1 FROM HOUR WHERE h < 23 
)

SELECT HOUR.h HOUR
     , COUNT(HOUR(o.DATETIME)) COUNT -- HOUR에 해당하는 DATETIME이 몇개인지 count
FROM HOUR
    LEFT JOIN ANIMAL_OUTS o ON HOUR.h = HOUR(o.DATETIME)
GROUP BY HOUR.h
ORDER BY HOUR.h
