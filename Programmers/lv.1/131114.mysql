SELECT WAREHOUSE_ID
     , WAREHOUSE_NAME
     , ADDRESS
     , IF(FREEZER_YN IS NULL, "N", FREEZER_YN) FREEZER_YN
FROM FOOD_WAREHOUSE
WHERE LEFT(ADDRESS, 3) = "경기도"
ORDER BY WAREHOUSE_ID

-- 240105
SELECT WAREHOUSE_ID
    , WAREHOUSE_NAME
    , ADDRESS
    , IF(FREEZER_YN IS NULL, 'N', FREEZER_YN) AS FREEZER_YN
FROM FOOD_WAREHOUSE
WHERE ADDRESS LIKE "경기도%"
ORDER BY WAREHOUSE_ID
