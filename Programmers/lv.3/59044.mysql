SELECT ANIMAL_INS.NAME
     , ANIMAL_INS.DATETIME
FROM ANIMAL_INS
    LEFT JOIN ANIMAL_OUTS ON ANIMAL_INS.ANIMAL_ID = ANIMAL_OUTS.ANIMAL_ID
WHERE ANIMAL_OUTS.DATETIME IS NULL
ORDER BY ANIMAL_INS.DATETIME ASC
LIMIT 3

-- 240111
SELECT NAME
    , DATETIME
FROM ANIMAL_INS
WHERE ANIMAL_ID NOT IN (
                        SELECT ANIMAL_ID
                        FROM ANIMAL_OUTS
                        )
ORDER BY DATETIME
LIMIT 3
