SELECT n.ANIMAL_ID
     , n.NAME
FROM ANIMAL_INS n
    INNER JOIN ANIMAL_OUTS o ON n.ANIMAL_ID = o.ANIMAL_ID
WHERE n.DATETIME > o.DATETIME
ORDER BY n.DATETIME

-- 240110
SELECT I.ANIMAL_ID
    , I.NAME
FROM ANIMAL_INS I
    INNER JOIN ANIMAL_OUTS O
    ON I.ANIMAL_ID = O.ANIMAL_ID
WHERE I.DATETIME > O.DATETIME -- 보호 시작일보다 입양일이 더 빠른
ORDER BY I.DATETIME
