WITH RANK_TABLE AS (
    SELECT FOOD_TYPE
         , REST_ID
         , REST_NAME
         , FAVORITES
         , RANK() OVER (PARTITION BY FOOD_TYPE ORDER BY FAVORITES DESC) AS FAV_RANK
    FROM REST_INFO
)

SELECT FOOD_TYPE
     , REST_ID
     , REST_NAME
     , FAVORITES
FROM RANK_TABLE
WHERE FAV_RANK = 1
ORDER BY FOOD_TYPE DESC
