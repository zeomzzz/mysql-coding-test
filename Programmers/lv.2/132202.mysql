SELECT MCDP_CD 진료과코드
     , COUNT(*) 5월예약건수
FROM APPOINTMENT
WHERE APNT_YMD BETWEEN '2022-05-01 00:00:00' AND '2022-05-31 23:59:59'
GROUP BY MCDP_CD
ORDER BY 5월예약건수, 
         진료과코드

-- 240107
SELECT MCDP_CD AS 진료과코드
    , COUNT(*) AS 월예약건수
FROM APPOINTMENT
WHERE YEAR(APNT_YMD) = 2022 
    AND MONTH(APNT_YMD) = 5
GROUP BY MCDP_CD
ORDER BY 월예약건수
    , MCDP_CD
