SELECT FLOOR(PRICE/10000) * 10000 AS PRICE_GROUP
     , COUNT(*) AS PRODUCTS
FROM PRODUCT
GROUP BY PRICE_GROUP
ORDER BY PRICE_GROUP

-- 241107
-- 만원 단위의 가격대 별로 상품 개수를 출력하는 SQL 문
SELECT FLOOR(PRICE/10000) * 10000 PRICE_GROUP
    , COUNT(PRODUCT_CODE) PRODUCTS
FROM PRODUCT
GROUP BY PRICE_GROUP
ORDER BY PRICE_GROUP
