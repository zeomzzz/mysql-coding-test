SELECT M.CATEGORY
     , M.MAX_PRICE
     , F.PRODUCT_NAME
FROM (SELECT CATEGORY, MAX(PRICE) MAX_PRICE
      FROM FOOD_PRODUCT
      GROUP BY CATEGORY
     ) M
    LEFT JOIN FOOD_PRODUCT F
              ON M.CATEGORY = F.CATEGORY
              AND M.MAX_PRICE = F.PRICE
WHERE M.CATEGORY IN ('과자', '국', '김치', '식용유')
ORDER BY M.MAX_PRICE DESC
